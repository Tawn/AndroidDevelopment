    <!--Button Styles-->
    <style name="CustomButtonStyle">
        <item name="android:textColor">@color/custom5</item>
        <item name="android:layout_margin">2dp</item>
        <item name="android:background">@drawable/custom_button_style2</item>
    </style>

</resources>